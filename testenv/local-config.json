{
  "Global": {
    "Port": "8080",
    "Hostname": "localhost"
    "TemplateDir": "templates/"
  },
  "Redirects": [
    {
      "Requested": "/favicon.ico",
      "Target": "/site/favicon.ico",
      "Code": 301
    },
    {
      "Requested": "/rawindex",
      "Target": "/raw/index.md",
      "Code": 307
    }
  ],
  "Server": [
    {
      "Path": ".",
      "Prefix": "/",
      "Default": "index",
      "ServerType": "markdown",
      "Template": "wiki.html",
      "Restricted": [
        "internal",
        "handbook"
      ]
    },
    {
      "Path": ".",
      "Prefix": "/raw/",
      "Default": {},
      "ServerType": "raw",
      "Restricted": [
        "md",
        "ini",
        "json"
      ]
    },
    {
      "Path": ".",
      "Prefix": "/site/",
      "Default": {},
      "ServerType": "raw",
      "Restricted": [
        "md",
        "ini",
        "json"
      ]
    }
  ]
}